package bracket;

public class BracketValidator {

	public static void main(String[] args) {
		
		 String[] input = args;
		 System.out.println(input[0]);
		 
		 char[] inputChars = args[0].toCharArray();
		 
		 for(int i = 0; i < inputChars.length; i++){
			 System.out.println(inputChars);
		 }

	}

}
