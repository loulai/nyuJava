import java.util.Scanner;


public class FindingLargest {

	public static void main(String[] args) {
		System.out.print("Type number: ");
		Scanner input = new Scanner(System.in);
		int max = 0;
		int num = 2;
	
		
		while (num != 0) {
			int num = input.nextInt();
			if (num > max){
				max = num;
			}
		}
		
		System.out.println(max);
		
	}

}
