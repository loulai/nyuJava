package temp;
import java.io.*;
import java.util.*;

public class temp2 {
	
	 // int[] a contains integers between 1 and 5
	  public static void findMissing(int[] a) {
	    int completeSum = a.length + 1;
	    int incompleteSum = 0;
	    int missingNumber;
	    
	    for(int i = 1; i <= a.length; i++){
	    	completeSum = completeSum + i;
	    }
	    
	    for (int i = 0; i < a.length; i++){
	    	incompleteSum = incompleteSum + a[i];
	    }
	    missingNumber = completeSum - incompleteSum;
	    System.out.println(completeSum);
	    System.out.println(incompleteSum);
	    System.out.printf("The missing number is %d.", missingNumber );
	   
	  }
	  
	  public static void main(String[] args) {
		    int[] numbers = {1, 3, 4, 5};
		    
		    findMissing(numbers);
	  }
	  
}
