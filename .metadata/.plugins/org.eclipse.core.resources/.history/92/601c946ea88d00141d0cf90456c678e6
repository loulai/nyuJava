package findSecondLargest;

public class SecondLargest {

	public static void main(String[] args) {
		int[] numArr = {1,2,3,4,5,6,3,9};
		int big1 = numArr[0];
		int big2 = numArr[0];
		
		for(int i = 0; i < numArr.length - 1; i++){
			if(numArr[i] < numArr[i + 1]) {
				big2 = big1;
				big1 = numArr[i + 1]; 
			}
		}
	}

}
