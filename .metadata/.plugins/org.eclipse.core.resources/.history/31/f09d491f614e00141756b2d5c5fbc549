import java.util.Scanner;


public class FindingLargest {

	public static void main(String[] args) {
		System.out.print("Type number: ");
		Scanner input = new Scanner(System.in);
		int num = input.nextInt();
		int max = 0;
		
		while (num != 0) {
			if (num > input.nextInt()){
				max = num;
			}
		}
		
	}

}
