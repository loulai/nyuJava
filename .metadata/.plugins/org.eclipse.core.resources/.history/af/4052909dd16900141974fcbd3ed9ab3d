
public class Test {

	public static void main(String[] args) {
		String s = new String("Hello");
		String s1 = "apples";
		String s2 = "z";
		
		String s3 = s1 + s2;
		
		System.out.println(s3);
		
		int i = s1.length();
		
		System.out.println(i);
		
		char c = s1.charAt(s1.length());
		
		System.out.println(c);
	}

}
