import java.util.Scanner;


public class Main {
	
	public static void main(String[] args) {
		for (int i = 1; i<=100; i = i+2) {
			if(i==3 || i==5 || i==7) {
				System.out.println(i);
				continue;
			} else if (i%3 != 0 && i%5 != 0 && i%7 != 0 && i != 1) {
				System.out.println(i);
			}
		}
	}
	
}

