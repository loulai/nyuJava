
public class Main {

	public static void main(String[] args) {
		
		int[] list1 = new int[10];
		
		System.out.println(list1[2]);
		
		for(int i = 0; i < list1.length; i++) {
			list1[i] = (int) (Math.random() * 1501);
		}
		
		for(int i = 0; i < list1.length; i++) {
			System.out.println((i + 1) + ". " + list1[i]);
		}
			
		
		
		
		
		
		
		
		
			
		}

	}

