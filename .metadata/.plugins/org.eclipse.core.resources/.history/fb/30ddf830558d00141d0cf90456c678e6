package findLargest;

import java.util.Scanner;

public class Largest {
	
	public static void main(String[] args){
		System.out.println("input string of integer without spaces, and indicate end by typing 0:");
		Scanner input = new Scanner(System.in);
		int largest = input.nextInt();
		int b = input.nextInt();
		
		System.out.printf("%d %d", largest, b);
	
		
	}
}

