import processing.core.PApplet;

public class ProcessingTest extends PApplet {	
	
	int timeCounter = 0;
	
	public void setup() {
		size(600,400);
		background(255);
		frameRate(2);
		Disk[] totalDisks = null;
		for(int i = 0; i < 8; i++){
			totalDisks[i] = new Disk(this);
		}
		
	}
	
	public void draw() {
		timeCounter++;
		
		if (timeCounter < 550) {
			background(100,100,10, 0);
		}
		
	}
}