package FinalQuestion11;

public class CalendarDate {

	private int day;
	private int month;
	public static void main(String[] args) {
		CalendarDate defaultD = new CalendarDate();
		CalendarDate biggerD = new CalendarDate();
		System.out.println(biggerD.compareTo(defaultD));
	}
	
	public CalendarDate(){
		this.day = 1;
		this.month = 5;
	}
	
	public int compareTo(CalendarDate c){
		if(this.month >= c.month && this.day > c.day ){
			return 1;
		} else if (this.month == c.month && this.day == c.day){
			return 0;
		} else return -1;
	}
	
	public int getDay(){
		return this.day;
	}

}
