
public class Sequence{

	public static void main(String[] args) {
		
		double n = 2;
		
		if (n % 2 == 0) {
			System.out.println("even");
		
			for (int i = 0; i < 100; i++) {
				System.out.print(n + ", ");
				n = n/2.0;
				System.out.print(n + ", ");
				if (i == 99) {
					System.out.print(n);
				}
			}
				
		}
		else {
			System.out.println("odd");
		}

	}

}
