package temp;
import java.io.*;
import java.util.*;
import java.util.Scanner;

public class temp2 {
	
	 // int[] a contains integers between 1 and 5
	  public static void findMissing(int[] a) {
	    int completeSum = a.length + 1;
	    int incompleteSum = 0;
	    int missingNumber;
	    
	    for(int i = 1; i <= a.length; i++){
	    	completeSum = completeSum + i;
	    }
	    
	    for (int i = 0; i < a.length; i++){
	    	incompleteSum = incompleteSum + a[i];
	    }
	    missingNumber = completeSum - incompleteSum;

	    System.out.printf("The missing number is %d.", missingNumber );
	   
	  }
	  
	  public static void main(String[] args) {
		  	System.out.println("Enter the largest number in your list: ");
		  	Scanner sizeInput = new Scanner(System.in);
		  	int upperBound = sizeInput.nextInt();
		  	
		  	System.out.printf("From 1 to %d (inclusive), which number would you like to remove?: \n", upperBound);
		  	
		    int[] numbers = {1, 3, 4, 5};
		    
		    findMissing(numbers);
	  }
	  
}
