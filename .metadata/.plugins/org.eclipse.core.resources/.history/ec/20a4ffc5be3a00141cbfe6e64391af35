
public class Main {

	public static void main(String[] args) {
		for (int i = 0; i < 6; i++){
			Double randNumber = Math.random() * 6 + 1;	
			System.out.printf(i + 1 + ". The random number is %f \n", randNumber);
			
			int tempRandNumber = randNumber.intValue();
			
			
			if (tempRandNumber == 1) {
				int oneCount = 0;
				oneCount++;
			}
		}
		
	}

}
