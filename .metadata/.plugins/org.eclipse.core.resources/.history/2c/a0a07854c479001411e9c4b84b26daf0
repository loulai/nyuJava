import processing.core.PApplet;

public class ProcessingTest extends PApplet {	
	public void setup() {
		size(600,400);
		background(255);
	}
	
	public void draw() {
		Disk disky = new Disk(this);

		ellipse(disky.x, disky.y, disky.rx, disky.ry);
		
		fill(random(0,200),random(0,10),random(0,200));
		frameRate(30);
	}
}