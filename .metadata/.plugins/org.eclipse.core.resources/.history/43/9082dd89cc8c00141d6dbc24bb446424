package hw1;

import java.util.Scanner;

public class FutureInterest {

	public static void main(String[] args) {
		
		System.out.println("investment amount, annual interest rate, number of years, in space separated order: ");
		Scanner in = new Scanner(System.in);
		
		int investmentAmount = in.nextInt();
		
		System.out.println(investmentAmount);
		
	}

}
