
public class Sequence{

	public static void main(String[] args) {
		
		double n = 2;
		
		if (n % 2 == 0) {
			System.out.println("even");
			System.out.println(n + ", ");
		
			for (int i = 0; i < 100; i++) {
				n = n/2.0;
				System.out.println(n + ", ");
			}
				
		}
		else {
			System.out.println("odd");
		}

	}

}
