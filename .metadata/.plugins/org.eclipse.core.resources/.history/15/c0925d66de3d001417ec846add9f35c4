
public class Sequence{

	public static void main(String[] args) {
		
		int n = 2;
		
		if (n % 2 == 0) {
			System.out.println("even");
		
			double value = n;
			
			for (int i = 0; i < 100; i++) {
				double value = value/2.0;
				System.out.println(n + ", ");
			}
				
		}
		else {
			System.out.println("odd");
		}

	}

}
