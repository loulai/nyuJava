package temp;
import java.io.*;
import java.util.*;

public class temp2 {
	
	 // int[] a contains integers between 1 and 5
	  public static void findMissing(int[] a) {
	    int missingNumber;
	    int sum = 0;
	    
	    for(int i = 0; i < a.length; i++){
	    	sum = sum + i;
	    }
	    System.out.println(sum);
	   
	  }
	  
	  public static void main(String[] args) {
		    int[] numbers = {5, 3, 1, 2};
		    
		    findMissing(numbers);
	  }
	  
}
