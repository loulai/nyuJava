import java.util.Scanner;


public class Main {
	
	public static void main(String[] args) {
		
		String[] names = {"Lou", "Jack", "Kala"};
		
		for (int i = 0; i < names.length; i ++) {
			System.out.println(names[i]);
			if(names[i].contains("Lou")) {
				System.out.println("Lou's in the class");
			}
		}
	
	}
	
}

