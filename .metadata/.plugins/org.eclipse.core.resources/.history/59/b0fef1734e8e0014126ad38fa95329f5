package hw5;

public class ShiftByOne {

	public static void main(String[] args){
		String s = "hello";
		//to       "ohell"
		char[] chars = s.toCharArray();
		char temp;
		
		temp = chars[1];
		chars[1] = chars[0];
		chars[2] = temp;
		
		String shifted = new String(chars);
		System.out.println(shifted);
	}

}
