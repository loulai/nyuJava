import processing.core.PApplet;

public class ProcessingTest extends PApplet {

	public void setup() {
		size(500,500);
	}

	public void draw() {
		if(mousePressed){
			fill(0);
		} else {
			fill(200);
		}
		ellipse(random(0,500), random(0,500),10,10);
	}
	
//	public void draw(){
//		triangle(random(0,300),random(0,300),random(0,300),random(0,10),random(0,300),random(0,300));
//	}
}
